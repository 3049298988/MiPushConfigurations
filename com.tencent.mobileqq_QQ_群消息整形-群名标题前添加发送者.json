{
  "version": "0.1.0",
  "configs": {
    "群消息整形-小米经典样式": [
      {
        "metaInfo": {
          "title": "(?<title>.*)",
          "description": "(?<name>.*?)[:：] \\s*(?<content>[\\s\\S]*)",
          "extra": {
            "intent_uri": "S.jumptype=57619"
          }
        },
        "newMetaInfo": {
          "title": "${name}•[${title}]",
          "description": "${content}",
          "extra": {
            "__mipush_tag_adjust": "1"
          }
        },
        "stop": false
      }
    ]
  }
}