{
  "version": "0.1.0",
  "configs": {
    "干掉运营消息": [
      {
        "metaInfo": {
          "extra": {
            "channel_id": null,
            "__mipush_tag_notify": null
          }
        },
        "operation": "ignore",
        "stop": false
      }
    ],
    "允许运营消息": [
      {
        "metaInfo": {
          "extra": {
            "channel_id": null
          }
        },
        "newMetaInfo": {
          "extra": {
            "__mipush_tag_notify": ""
          }
        },
        "stop": false
      }
    ],
    "禁止通知成组": [
      {
        "metaInfo": {
          "id": "(?<id>.*)"
        },
        "newMetaInfo": {
          "extra": {
            "notification_group": "${id}"
          }
        },
        "stop": false
      }
    ],
    "按标题成组": [
      {
        "metaInfo": {
          "title": "(?<title>.*)"
        },
        "newMetaInfo": {
          "extra": {
            "notification_group": "${title}"
          }
        },
        "stop": false
      }
    ],
    "通用消息内容解析": [
      [
        "cond",
        [
          "群组消息",
          {
            "metaInfo": {
              "title": "(?<title>.*)",
              "description": "(?<name>.*?)[:：]\\s*(?<content>[\\s\\S]*)",
              "extra": {
                "__mi_push_conversation_message": null
              }
            },
            "newMetaInfo": {
              "extra": {
                "__mi_push_conversation_title": "${title}",
                "__mi_push_conversation_sender": "${name}",
                "__mi_push_conversation_message": "${content}"
              }
            },
            "stop": false
          }
        ],
        [
          "个人消息",
          {
            "metaInfo": {
              "title": "(?<title>.*)",
              "description": "(?<content>[\\s\\S]*)",
              "extra": {
                "__mi_push_conversation_message": null
              }
            },
            "newMetaInfo": {
              "extra": {
                "__mi_push_conversation_sender": "${title}",
                "__mi_push_conversation_message": "${content}"
              }
            },
            "stop": false
          }
        ]
      ]
    ],
    "使用 Messaging Style": [
      "通用消息内容解析",
      {
        "metaInfo": {
          "extra": {
            "__mi_push_conversation_message": ""
          }
        },
        "newMetaInfo": {
          "extra": {
            "__mi_push_use_messaging_style": ""
          }
        },
        "stop": false
      }
    ],
    "[Messaging Style] 大图标作为个人头像": [
      {
        "metaInfo": {
          "extra": {
            "__mi_push_conversation_message": "",
            "notification_large_icon_uri": "(?<icon>[\\s\\S]+)"
          }
        },
        "newMetaInfo": {
          "extra": {
            "__mi_push_use_messaging_style": "",
            "__mi_push_conversation_sender_icon": "${icon}",
            "notification_large_icon_uri": null
          }
        },
        "stop": false
      }
    ],
    "群名作为subtext展示": [
      "通用消息内容解析",
      {
        "metaInfo": {
          "extra": {
            "__mi_push_conversation_title": "(?<title>[\\s\\S]*)",
            "__mi_push_conversation_sender": "(?<sender>[\\s\\S]*)",
            "__mi_push_conversation_message": "(?<content>[\\s\\S]*)"
          }
        },
        "newMetaInfo": {
          "title": "${sender}",
          "description": "${content}",
          "extra": {
            "__mi_push_sub_text": "${title}"
          }
        },
        "stop": false
      }
    ],
    "移除通知副标题": [
      {
        "metaInfo": {
          "extra": {
            "__mi_push_sub_text": null,
            "__mipush_tag_sub": null
          }
        },
        "newMetaInfo": {
          "extra": {
            "__mi_push_sub_text": ""
          }
        },
        "stop": false
      }
    ],
    "允许通知副标题": [
      {
        "metaInfo": {
          "extra": {
            "__mi_push_sub_text": null
          }
        },
        "newMetaInfo": {
          "extra": {
            "__mipush_tag_sub": ""
          }
        },
        "stop": false
      }
    ],
    "大图标显示成圆形": [
      {
        "newMetaInfo": {
          "extra": {
            "__mi_push_round_large_icon": ""
          }
        },
        "stop": false
      }
    ],
    "标记为群组消息": [
      {
        "newMetaInfo": {
          "extra": {
            "__mipush_tag_char_type": "group"
          }
        },
        "stop": false
      }
    ],
    "群组消息": [
      {
        "metaInfo": {
          "extra": {
            "__mipush_tag_char_type": "group"
          }
        },
        "stop": false
      }
    ],
    "标记为个人消息": [
      {
        "newMetaInfo": {
          "extra": {
            "__mipush_tag_char_type": "personal"
          }
        },
        "stop": false
      }
    ],
    "个人消息": [
      {
        "metaInfo": {
          "extra": {
            "__mipush_tag_char_type": "personal"
          }
        },
        "stop": false
      }
    ],
    "将标题相同的通知视为同一会话": [
      {
        "metaInfo": {
          "title": "(?<title>[\\s\\S]*)"
        },
        "newMetaInfo": {
          "notifyId": [
            "hash",
            [
              "$",
              "title"
            ]
          ]
        },
        "stop": false
      }
    ]
  }
}